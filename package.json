{
  "name": "rocket-rental",
  "private": true,
  "sideEffects": false,
  "license": "GPL-3.0-only",
  "author": "Kent C. Dodds <me@kentcdodds.com> (https://kentcdodds.com/)",
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "build:server": "tsx ./other/build-server.ts",
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:server": "cross-env MOCKS=true tsx watch --clear-screen=false ./index.js",
    "dev:remix": "remix watch",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./index.js",
    "start:mocks": "cross-env NODE_ENV=production MOCKS=true tsx ./index.js",
    "test": "vitest",
    "test:e2e:dev": "playwright test",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env CI=true playwright test",
    "test:e2e:install": "npx playwright install chromium --with-deps",
    "typecheck": "tsc",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "proseWrap": "always",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": false,
    "singleAttributePerLine": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": true
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/playwright-report"
  ],
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "@remix-run/express": "*",
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "@remix-run/server-runtime": "*",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^8.0.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "fs-extra": "^11.1.0",
    "isbot": "^3.6.5",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.4.0",
    "remix-auth-form": "^1.3.0",
    "remix-params-helper": "^0.4.10",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@playwright/test": "^1.29.2",
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/better-sqlite3": "^7.6.3",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.4.10",
    "@types/express": "^4.17.16",
    "@types/fs-extra": "^11.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitest/coverage-c8": "^0.27.1",
    "c8": "^7.12.0",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "happy-dom": "^8.1.4",
    "msw": "^0.49.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "prisma": "^4.9.0",
    "remix-flat-routes": "^0.5.6",
    "start-server-and-test": "^1.15.2",
    "tailwindcss": "^3.2.4",
    "tsconfig-paths": "^4.1.2",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "^0.27.1"
  },
  "engines": {
    "node": "18"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
